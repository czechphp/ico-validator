{
    "name": "czechphp/ico-validator",
    "type": "library",
    "description": "Validator for IČO or taxpayer identification number in the Czech Republic",
    "keywords": [
        "taxpayer identification number",
        "validator",
        "Czech republic",
        "Czechia",
        "IČO",
        "Identifikační číslo osoby"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "authors": [
        {
            "name": "Lukáš Mencl",
            "email": "tetra.jinak@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Czechphp\\ICOValidator\\": ""
        },
        "exclude-from-classmap": [
            "Tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Czechphp\\ICOValidator\\Tests\\": "Tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
